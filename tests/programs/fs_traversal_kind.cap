module fs_traversal_kind
use sys::system
use sys::fs

pub fn main(sys: RootCap) -> i32 {
  let c = sys.mint_console()
  let rfs = sys.mint_readfs("./config")

  match rfs.read_to_string("../secrets.txt") {
    Ok(s) => { c.println("BUG: escaped"); return 2 }
    Err(e) => {
      if (e == fs::FsErr::InvalidPath) { c.println("invalid"); return 0 }
      c.println("wrong"); return 1
    }
  }
}
