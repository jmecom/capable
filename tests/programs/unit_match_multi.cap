module unit_match_multi
use sys::system

fn helper() -> unit {
  return ()
}

pub fn main(rc: RootCap) -> i32 {
  let c = rc.mint_console()
  // Test that match arms can return unit
  // Match on a bool, but both arms return unit
  let flag = 0
  match true {
    true => {
      helper()
      flag = 1
    }
    false => {
      helper()
      flag = 2
    }
  }
  c.assert(flag == 1)
  c.println("unit match multi ok")
  return 0
}
