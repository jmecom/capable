module unit_match_multi
use sys::system
use sys::console

fn helper() -> unit {
  return ()
}

pub fn main(sys: RootCap) -> i32 {
  let c = sys.mint_console()
  // Test that match arms can return unit
  // Match on a bool, but both arms return unit
  match true {
    true => { helper() }
    false => { helper() }
  }
  console::println(c, "unit match multi ok")
  return 0
}
