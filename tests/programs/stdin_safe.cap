package safe
module stdin_safe
use sys::system

pub fn main(sys: RootCap) -> i32 {
  let c = sys.mint_console()
  let code = match sys.read_stdin_to_string() {
    Ok(s) => {
      let n = s.len()
      sys.assert(n == 0)
      if (n == 0) {
        c.println("stdin ok")
      } else {
        c.println("stdin data")
      }
      0
    }
    Err(e) => {
      c.println("stdin err")
      1
    }
  }
  return code
}
