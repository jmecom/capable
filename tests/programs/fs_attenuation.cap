module fs_attenuation
use sys::system

pub fn main(rc: RootCap) -> i32 {
  let c = rc.mint_console()
  let alloc = rc.mint_alloc_default()
  let fs = rc.mint_filesystem("./config")
  let root = fs.root_dir()
  let file = root.open_read("app.txt")

  match file.read_to_string(alloc) {
    Ok(s) => { c.println(s); return 0 }
    Err(e) => { c.println("read failed"); return 1 }
  }
}
