module fs_read
use sys::system
use sys::fs
use sys::console

pub fn main(sys: RootCap) -> i32 {
  let c = sys.mint_console()
  let rfs = sys.mint_readfs("./config")

  match fs::read_to_string(rfs, "app.txt") {
    Ok(s) => { console::println(c, s); return 0 }
    Err(e) => { console::println(c, "read failed"); return 1 }
  }
}
