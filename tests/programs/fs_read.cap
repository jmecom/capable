module fs_read
use sys::root
use sys::fs
use sys::console

pub fn main(rc: RootCap) -> i32 {
  let c = rc.mint_console()
  let rfs = rc.mint_fs_read("./config")

  match rfs.read_to_string("app.txt") {
    Ok(s) => { c.println(s); return 0 }
    Err(e) => { c.println("read failed"); return 1 }
  }
}
