module fs_read
use sys::system

pub fn main(rc: RootCap) -> i32 {
  let c = rc.mint_console()
  let alloc = rc.mint_alloc_default()
  let rfs = rc.mint_readfs("./config")

  match rfs.read_to_string(alloc, "app.txt") {
    Ok(s) => { c.println(s); return 0 }
    Err(e) => { c.println("read failed"); return 1 }
  }
}
