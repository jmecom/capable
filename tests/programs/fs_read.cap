module app
use sys.system
use sys.fs
use sys.console

pub fn main(sys: System) -> i32 {
  let c = system.console(sys)
  let rfs = system.fs_read(sys, "./config")

  match fs.read_to_string(rfs, "app.txt") {
    Ok(s) => { console.println(c, s); return 0 }
    Err(e) => { console.println(c, "read failed"); return 1 }
  }
}
