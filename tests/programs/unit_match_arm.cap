module unit_match_arm
use sys::system

pub fn main(sys: RootCap) -> i32 {
  let c = sys.mint_console()
  let flag = 0
  match true {
    true => {
      flag = 1
      c.println("unit match arm ok")
    }
    false => {
      flag = 2
      c.println("wrong branch")
    }
  }
  sys.assert(flag == 1)
  return 0
}
