module match_expr
use sys::system

pub fn main(rc: RootCap) -> i32 {
  let c = rc.mint_console()
  let v = match true {
    true => { "yes" }
    false => { "no" }
  }
  c.assert(
    v.len() == 3 &&
    v.byte_at(0) == 'y' &&
    v.byte_at(1) == 'e' &&
    v.byte_at(2) == 's'
  )
  c.println(v)
  return 0
}
