package safe
module args_safe
use sys::system

pub fn main(sys: RootCap) -> i32 {
  let c = sys.mint_console()
  let n = sys.args_len()
  sys.assert(n >= 1)
  if (n < 1) {
    c.println("args bad")
    return 1
  }
  let code = match sys.args_at(0) {
    Ok(s) => {
      let m = s.len()
      sys.assert(m >= 0)
      if (m >= 0) {
        c.println("args ok")
      }
      0
    }
    Err(e) => {
      c.println("args err")
      1
    }
  }
  return code
}
