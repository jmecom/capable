package safe
module args_safe
use sys::root
use sys::console
use sys::args
use sys::string

pub fn main(rc: RootCap) -> i32 {
  let c = rc.mint_console()
  let a = rc.mint_args()
  let n = a.len()
  if (n < 1) {
    c.println("args bad")
    return 1
  }
  let code = match a.at(0) {
    Ok(s) => {
      let m = s.len()
      if (m >= 0) {
        c.println("args ok")
      }
      0
    }
    Err(e) => {
      c.println("args err")
      1
    }
  }
  return code
}
