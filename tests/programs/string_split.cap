package safe
module string_split
use sys::system

pub fn main(sys: RootCap) -> i32 {
  let c = sys.mint_console()
  let parts = "a,b,c".split(44u8)
  let count = parts.len()
  let alloc = sys.alloc_default()
  alloc.vec_string_free(parts)
  if (count == 3) {
    c.println("split ok")
    return 0
  }
  c.println("split bad")
  return 1
}
