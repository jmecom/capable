module early_return_block
use sys.system
use sys.console

pub fn main(sys: System) -> i32 {
  let c = system.console(sys)

  if (true) {
    console.println(c, "before return")
    console.println(c, "early return test ok")
    return 0
    console.println(c, "after return - SHOULD NOT PRINT")
  }

  console.println(c, "after if - SHOULD NOT PRINT")
  return 1
}
