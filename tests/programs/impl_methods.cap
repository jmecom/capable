module impl_methods
use sys::system
use sys::console

struct Pair { left: i32, right: i32 }

impl Pair {
  fn sum(self) -> i32 {
    return self.left + self.right
  }

  fn add(self, value: i32) -> i32 {
    return self.left + value
  }
}

pub fn main(sys: RootCap) -> i32 {
  let c = sys.mint_console()
  let p = Pair { left: 2, right: 5 }
  let a = p.sum()
  let b = Pair { left: 7, right: 3 }.add(10)
  console::print_i32(c, a)
  console::print(c, " ")
  console::print_i32(c, b)
  console::println(c, "")
  return 0
}
