module early_return_while
use sys::system

pub fn main(sys: RootCap) -> i32 {
  let c = sys.mint_console()

  let cond = true
  let hit = 0
  while (cond) {
    hit = 1
    sys.assert(hit == 1)
    c.println("loop iteration")
    c.println("early return while test ok")
    return 0
    c.println("after return in loop - SHOULD NOT PRINT")
  }

  c.println("after loop - SHOULD NOT PRINT")
  return 1
}
