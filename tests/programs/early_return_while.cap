module early_return_while
use sys.system
use sys.console

pub fn main(sys: System) -> i32 {
  let c = system.console(sys)

  let cond = true
  while (cond) {
    console.println(c, "loop iteration")
    console.println(c, "early return while test ok")
    return 0
    console.println(c, "after return in loop - SHOULD NOT PRINT")
  }

  console.println(c, "after loop - SHOULD NOT PRINT")
  return 1
}
