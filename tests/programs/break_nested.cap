module break_nested
use sys::system
use sys::console

pub fn main(rc: RootCap) -> i32 {
  let c = rc.mint_console()
  let outer_count = 0
  let inner_sum = 0
  let i = 0
  while (i < 5) {
    let j = 0
    while (j < 10) {
      if (j == 3) {
        break
      }
      inner_sum = inner_sum + 1
      j = j + 1
    }
    outer_count = outer_count + 1
    i = i + 1
  }
  if (outer_count == 5) {
    if (inner_sum == 15) {
      c.println("break nested ok")
    }
  }
  return 0
}
