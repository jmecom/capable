package safe
module string_helpers
use sys.system
use sys.console
use sys.string
use sys.bytes

pub fn main(sys: System) -> i32 {
  let c = system.console(sys)
  let s = "abc"
  let buf = string.bytes(s)
  let n = bytes.len(buf)
  let b = bytes.at(buf, 0)
  if (n == 3 && b == 97u8) {
    console.println(c, "string ok")
    return 0
  }
  console.println(c, "string bad")
  return 1
}
